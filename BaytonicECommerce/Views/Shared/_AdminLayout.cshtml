        <script type="text/javascript">

            function keepAliveFunc() {

                setTimeout("keepAlive()", 60000);

            };

            function keepAlive() {
            $.post('@Url.Action("KeepSessionAlive", "Home")', null, function () { keepAliveFunc(); });

            };
            $(keepAliveFunc());

